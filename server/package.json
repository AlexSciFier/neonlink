{
  "name": "neonlink-backend",
  "version": "1.0.0",
  "description": "NeonLink backend part bootstrapped with Fastify-CLI.",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap \"test/**/*.test.js\"",
    "start": "fastify start -a \"0.0.0.0\" -l info --body-limit 5242880 app.js",
    "dev": "fastify start -a \"0.0.0.0\" -w --ignore-watch='db/bookmarks.sqlite' --body-limit 5242880 -l info -P app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/static": "^5.0.2",
    "axios": "^0.26.1",
    "better-sqlite3": "^7.5.3",
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.10.0",
    "fastify-cli": "^2.15.0",
    "fastify-cookie": "^5.6.0",
    "fastify-cors": "^6.0.3",
    "fastify-file-upload": "^4.0.0",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.2",
    "fastify-session": "^5.2.1",
    "node-html-parser": "^5.3.3"
  },
  "devDependencies": {
    "tap": "^16.3.0"
  }
}
